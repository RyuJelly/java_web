<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=EUC-KR">
<title>Insert title here</title>
<script type="text/javascript">
	// onmouseover
	// onmouseout
	
	var defaultImg = "../images/cat0.jpg";
	
	function over(){
		document.defaultt.src = this.src;
	}
	
	function out(){
		document.defaultt.src = defaultImg;
	}
	
	window.onload = function(){
		var imgs = document.getElementsByTagName("img");
		for(var i=0; i<imgs.length; i++){
			imgs[i].onmouseover = over;
			imgs[i].onmouseout = out;
		}
	}
	
	
</script>
</head>

<body>
<center>
	<h2>고양이 앨범</h2>
	<img id="defaultt" src="../images/cat0.jpg" border="1" width="600" height="450"></img>
	<br/><br/><br/>
	
	<img src="../images/cat1.jpg" width="70" height="70" border="1" hspace="10" 
			vspace="10" >
	<img src="../images/cat2.jpg" width="70" height="70" border="1" hspace="10" 
			vspace="10">
	<img src="../images/cat3.jpg" width="70" height="70" border="1" hspace="10" 
			vspace="10">
	
	
</center>
</body>
</html>