<script type="text/javascript">
	// 사용자 정의 객체
	
	// 1. Object 객체 이용 => 동적 프로퍼티 추가 가능
	var obj = new Object();
	obj.name = "홍길동";
	obj.age = 20;
	
	console.log("이름: " + obj.name);
	console.log("나이: " + obj.age);

	// 2. 리터럴 형식 객체
	var a = {};
	var obj2 = {
		name: "박길동",
		age: 30,
		display: function(){
			console.log(this.name);
		}
	};
	
</script>
