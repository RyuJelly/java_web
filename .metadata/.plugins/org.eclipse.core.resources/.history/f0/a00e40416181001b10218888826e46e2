	//-------------------------------------------------------
	
	class Rectangle{
		constructor(w, h){
			this.width = w;
			this.height = h;
		}
		
		setWidth(w){
			width = w;
		}
		getWidth(){
			return width;
		}
		setHeight(h){
			height = h;
		}
		getHeight(){
			return height;
		}
		
		getArea(){
			return this.width * this.height;
		}
	}
	
	// 객체생성
	var rectangle = new Rectangle(5, 7);
	rectangle.setWidth(10);
	
	// 메서드 호출: 넓이 출력
	console.log(rectangle.getArea());

</script>
