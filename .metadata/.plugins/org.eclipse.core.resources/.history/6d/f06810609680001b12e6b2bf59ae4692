<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
<script type="text/javascript">
	/* JavaScript 자료형 */
	// 숫자형(number)
	var intNum = 10;
	var floatNum = 3.14;
	
	console.log(typeof intNum);
	console.log(typeof floatNum);
	
	// 문자형(string)
	var sinS = 'single';
	var doubleS = 'double';
	
	console.log(typeof sinS);
	console.log(typeof doubleS);

	// 논리형(boolean)
	var boolVar = true;	
	console.log(typeof boolVar);
	
	// undefined
	var emptyVar	
	console.log(typeof emptyVar);
	
	// null(object)
	var nullVar = null;
	console.log(typeof nullVar);
	
	// 함수(function)
	var func = function(){};
	console.log(typeof func);
	
</script>
</head>
<body>

</body>
</html>