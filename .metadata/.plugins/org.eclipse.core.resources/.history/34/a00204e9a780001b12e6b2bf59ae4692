<script type="text/javascript">

	//console.log("add: " + add(10,20));
	//console.log("add2: " + add2(10,20));
	//console.log(typeof add2);
	
	// 선언적 함수
	function add(a,b){
		var sum = a + b;
		return sum;
	}
	
	// 익명함수
	var add2 = function(a,b){
		var sum = a * b;
		return sum;
	}
	
	// 변수에 함수를 전달할 수 있다.
	var plus = add2;
	console.log(plus(10,5));
	


</script>
