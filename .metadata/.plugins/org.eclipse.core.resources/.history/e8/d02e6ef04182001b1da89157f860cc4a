<script type="text/javascript">
	var data = 10;
	
	function(){
		this.data = 20;
		data = 30;
		
		console.log("1. data: " + data);	// 30
		console.log("2. this.data: " + this.data);	// 30
		console.log("1. window.data: " + window.data); // 20
	}

</script>
</head>
