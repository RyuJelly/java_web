	//----------------------------------------------------
	
	function Rectangle(w, h){
		// 가로, 세로
		this.w = w;
		this.h = h;
		
		this.setWidth = function(w){
			w = width;
		}
		this.getWidth = function(){
			return w;
		}
		this.setHeight = function(h){
			h = height;
		}
		this.getWidth = function(){
			return h;
		}
	}
	
	Rectangle.prototype = {
			getArea: function(){
				return this.w * this.h;
			}
	}
	
	// 객체생성
	var rectangle = new Rectangle(5, 7);
	rectangle.setWidth(10);
	
	// 메서드 호출: 넓이 출력
	console.log(rectangle.getArea());

</script>
